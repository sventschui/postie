{
  "name": "@axah/postie-web",
  "version": "3.0.1",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build/**"
  ],
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --template src/index.html --no-prerender && pnpm build-index",
    "build-index": "tsc --build tsconfig.index.json",
    "serve": "preact build --template src/index.html --no-prerender && serve build --single",
    "standalone-dev": "preact watch --template src/index.html",
    "lint": "eslint src",
    "codegen": "graphql-codegen",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf build"
  },
  "devDependencies": {
    "@axah/eslint-config": "^2.0.0",
    "@axah/tsconfig": "^1.1.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@types/node": "^20.4.0",
    "babel-plugin-graphql-tag": "^3.3.0",
    "eslint": "^8.44.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.4.6",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@urql/exchange-graphcache": "^6.1.4",
    "@urql/preact": "^4.0.3",
    "graphql": "^16.7.1",
    "graphql-ws": "^5.14.0",
    "preact": "^10.15.1",
    "preact-render-to-string": "^6.1.0",
    "preact-router": "^4.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "styled-jsx": "^5.1.2",
    "urql": "^4.0.4"
  }
}
